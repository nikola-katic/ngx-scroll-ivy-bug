import { OnInit, OnDestroy, NgZone, ElementRef } from '@angular/core';
import { Platform } from '@angular/cdk/platform';
import { Directionality } from '@angular/cdk/bidi';
import { Subject } from 'rxjs';
import { NgScrollbar } from '../ng-scrollbar';
export declare class ScrollbarControl implements OnInit, OnDestroy {
    parent: NgScrollbar;
    private platform;
    private dir;
    private zone;
    private document;
    private scrollbarRef;
    private track;
    trackRef: ElementRef;
    thumbRef: ElementRef;
    protected readonly destroyed: Subject<void>;
    constructor(parent: NgScrollbar, platform: Platform, dir: Directionality, zone: NgZone, document: any);
    ngOnInit(): void;
    ngOnDestroy(): void;
}
